{
  "name": "futuractive-web",
  "version": "0.1.0",
  "private": true,
  "description": "A brand new meetup for all things React, JavaScript and Functional (Reactive) Programming!",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf lib static .happypack",
    "build:assets": "rm -rf ./static && cp -R ./src/assets ./static",
    "build:server": "babel src/server --out-dir lib/server",
    "build:client": "webpack --config webpack.config.js",
    "build:production": "NODE_ENV=production ./scripts/build",
    "build": "NODE_ENV=development ./scripts/build",
    "start:production": "NODE_ENV=production ./scripts/server",
    "start": "NODE_ENV=development ./scripts/server"
  },
  "dependencies": {
    "goethe": "^2.0.0",
    "google-map-react": "^0.22.3",
    "native-promise-only": "^0.8.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.2",
    "sanitize.css": "^4.1.0",
    "styled-components": "next"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-styled-components": "^1.0.0",
    "babel-plugin-transform-react-constant-elements": "^6.22.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.12",
    "babel-preset-react-app": "^2.0.1",
    "core-js": "^2.4.1",
    "css-loader": "^0.26.1",
    "dotenv": "^2.0.0",
    "eslint": "^3.9.1",
    "eslint-config-excellence": "^1.17.1",
    "eslint-plugin-react": "^6.6.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "2.0.0-rc.2",
    "happypack": "^3.0.2",
    "json-loader": "^0.5.4",
    "react-hot-loader": "3.0.0-beta.6",
    "redbox-react": "^1.3.3",
    "style-loader": "^0.13.1",
    "svg-inline-loader": "^0.7.1",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-disk-plugin": "^0.0.2",
    "webpack-hot-middleware": "^2.16.1"
  }
}
