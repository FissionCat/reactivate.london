{
  "name": "futuractive-web",
  "version": "0.1.0",
  "private": true,
  "description": "A brand new meetup for all things React, JavaScript and Functional (Reactive) Programming!",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf lib static .happypack",
    "build:assets": "mkdir -p static || true; cp -R src/assets/* static/",
    "build:server": "cross-env BABEL_ENV=commonjs babel src/server --out-dir lib/server",
    "build:client": "cross-env BABEL_ENV=production webpack --config webpack.config.js",
    "production": "cross-env NODE_ENV=production ./scripts/server",
    "start": "cross-env NODE_ENV=development ./scripts/server"
  },
  "dependencies": {
    "goethe": "^1.2.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "sanitize.css": "^4.1.0",
    "styled-components": "^1.0.10"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.3.21",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.0",
    "babel-plugin-module-resolver": "^2.3.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.10",
    "babel-preset-philpl": "^0.4.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.25.0",
    "dotenv": "^2.0.0",
    "eslint": "^3.9.1",
    "eslint-config-excellence": "^1.17.1",
    "eslint-plugin-react": "^6.6.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "happypack": "^2.2.1",
    "json-loader": "^0.5.4",
    "react-hot-loader": "^3.0.0-beta.5",
    "redbox-react": "^1.3.1",
    "style-loader": "^0.13.1",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.2",
    "write-file-webpack-plugin": "^3.4.2"
  }
}
